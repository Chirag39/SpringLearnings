<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="
           http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans-3.1.xsd
           http://www.springframework.org/schema/context
           http://www.springframework.org/schema/context/spring-context-3.1.xsd">
    
    <context:annotation-config/>
   
    <bean id="customer" class="org.q5.Customer">
    <property name="customerName" value="Max"></property>
    <property name="customerId" value="33"></property>
    <property name="customerContact" value="987654321"></property>

    </bean>
    
    <bean id="address" class="org.q5.Address">
    <property name="street" value="Ch-road"></property>
    <property name="city" value="Mumbai"></property>
    <property name="state" value="Maharashtra"></property>
    <property name="zip" value="400002"></property>
    <property name="country" value="India"></property>
    </bean>
    
    
    
    </beans>